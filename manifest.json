{
  "name": "Instant Scholar",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*.pdf",
        "https://*/*.pdf"
      ],
      "js": [
        "dist/main.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "storage"
  ]
}
